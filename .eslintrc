{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "quotes": "off",
    "arrow-parens": "off",
    "no-unused-vars": "off",
    "indent": "off",
    "comma-dangle": "off",
    "no-trailing-spaces": "off",
    "max-len": "off",
    "curly": "off",
    "linebreak-style": "off"
  },
  "extends": ["eslint:recommended", "google"]
}